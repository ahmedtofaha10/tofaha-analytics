{
    "name": "tofaha/tofaha-analytics",
    "description": "Laravel Analytics Awesome package",
    "keywords": [
        "tofaha",
        "tofaha-analytics"
    ],
    "homepage": "https://github.com/ahmedtofaha10/tofaha-analytics",
    "license": "MIT",
    "type": "library",
    "version": "0.1",
    "authors": [
        {
            "name": "ahmedtofaha10",
            "email": "ahmedtofaha999@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tofaha\\TofahaAnalytics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tofaha\\TofahaAnalytics\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tofaha\\TofahaAnalytics\\TofahaAnalyticsServiceProvider"
            ],
            "aliases": {
                "TofahaAnalytics": "Tofaha\\TofahaAnalytics\\TofahaAnalyticsFacade"
            }
        }
    }
}
